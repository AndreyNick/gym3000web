<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="dao_services_configs_context.xml"/>

    <!--Objects-->
    <bean id="exerciseTemplate" class="com.gym.objects.ExerciseTemplate">
        <constructor-arg index="0" value="exerciseTemplate"/>
    </bean>
    <bean id="programTemplate" class="com.gym.objects.ProgramTemplate">
        <constructor-arg index="0" value="programTemplate"/>
    </bean>
    <bean id="exercise" class="com.gym.objects.Exercise">
        <constructor-arg index="0" value="exercise"/>
    </bean>
    <bean id="program" class="com.gym.objects.Program">
        <constructor-arg index="0" value="program"/>
        <constructor-arg index="1">
            <bean class="java.sql.Date" factory-method="valueOf">
                <constructor-arg value="2016-04-14"/>
            </bean>
        </constructor-arg>
    </bean>
</beans>